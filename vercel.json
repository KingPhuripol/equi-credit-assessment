{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "web/$1"
    }
  ],
  "env": {
    "TYPHOON_API_KEY": "@typhoon_api_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}
